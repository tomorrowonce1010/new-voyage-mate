# ========================================
# VoyageMate 项目环境变量模板
# ========================================
# 请复制此文件为 .env 并填入真实的配置值
# 注意：.env 文件已被 .gitignore 忽略，不会提交到仓库

# ========================================
# 后端配置 (Backend)
# ========================================

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=voyagemate
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# DeepSeek AI 配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions

# ========================================
# 前端配置 (Frontend)
# ========================================

# 高德地图配置
REACT_APP_AMAP_KEY=your_amap_key_here
REACT_APP_AMAP_SECURITY_CODE=your_amap_security_code_here

# ========================================
# Python 服务配置
# ========================================

# DeepSeek API (Python服务)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Elasticsearch 配置
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_INDEX=attractions

# MySQL 配置 (Python服务)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=voyagemate
MYSQL_USER=your_db_username
MYSQL_PASSWORD=your_db_password
